# Environment Variables for Maestro Tests
# Copy this file to .maestro/.env and fill in your values

# App identifiers
APP_ID=com.yourcompany.yourapp
APP_ID_IOS=com.yourcompany.yourapp
APP_ID_ANDROID=com.yourcompany.yourapp

# Test credentials
TEST_EMAIL=test@example.com
TEST_PASSWORD=testpassword123

# API endpoints (if needed for mocking)
API_BASE_URL=https://api.example.com
TEST_API_KEY=your-test-api-key

# Test data
TEST_USER_ID=12345
TEST_USERNAME=testuser

# Feature flags (if applicable)
ENABLE_FEATURE_X=true
ENABLE_FEATURE_Y=false

# Timeouts (in milliseconds)
DEFAULT_TIMEOUT=10000
NETWORK_TIMEOUT=15000

# Platform-specific settings
IOS_SIMULATOR=iPhone 15 Pro
ANDROID_EMULATOR=Pixel_7_API_34
