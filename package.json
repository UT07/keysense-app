{
  "name": "purrrfect-keys",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:build:ios": "detox build -c ios.sim.debug",
    "e2e:test:ios": "detox test -c ios.sim.debug --cleanup",
    "e2e:test:ios:headless": "detox test -c ios.sim.debug --cleanup --headless",
    "validate:exercises": "ts-node scripts/validate-exercises.ts",
    "generate:exercise": "ts-node scripts/generate-exercise.ts",
    "measure:latency": "ts-node scripts/measure-latency.ts",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:preview": "eas build --profile preview --platform ios",
    "build:preview-sim": "eas build --profile preview-simulator --platform ios",
    "build:local-release": "npx expo run:ios --configuration Release",
    "maestro:smoke": "maestro test .maestro/flows/01-smoke-test.yaml",
    "maestro:all": "maestro test .maestro/flows/",
    "maestro:critical": "maestro test --include-tags=critical .maestro/flows/"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@motiz88/react-native-midi": "^0.0.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@shopify/react-native-skia": "1.5.0",
    "abcjs": "^6.6.2",
    "expo": "~52.0.0",
    "expo-apple-authentication": "~7.1.3",
    "expo-asset": "^55.0.8",
    "expo-av": "~15.0.2",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-file-system": "~18.0.12",
    "expo-gl": "^15.0.5",
    "expo-haptics": "~14.0.0",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.0",
    "expo-screen-orientation": "~8.0.4",
    "expo-sharing": "~13.0.1",
    "expo-speech": "~13.0.1",
    "expo-splash-screen": "~0.29.5",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.27.5",
    "firebase": "^12.9.0",
    "onnxruntime-react-native": "^1.24.2",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.9",
    "react-native-audio-api": "^0.9.3",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.8.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-web": "^0.19.13",
    "three": "^0.170.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.47.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~8.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.6.0"
  },
  "resolutions": {
    "metro": "0.81.0",
    "metro-core": "0.81.0",
    "metro-config": "0.81.0",
    "metro-resolver": "0.81.0",
    "metro-runtime": "0.81.0"
  }
