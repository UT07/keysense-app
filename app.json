{
  "expo": {
    "name": "Purrrfect Keys",
    "slug": "purrrfect-keys",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.purrrfectkeys.app",
      "buildNumber": "1",
      "usesAppleSignIn": true,
      "googleServicesFile": "./firebase/GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.619761780367-tqf3t4srqtkklkigep0clojvoailsteu"
            ]
          }
        ],
        "NSMicrophoneUsageDescription": "Purrrfect Keys uses your microphone to detect the notes you play on piano, keyboard, or by singing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0D0D"
      },
      "package": "com.purrrfectkeys.app",
      "googleServicesFile": "./firebase/google-services.json",
      "versionCode": 1,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-dev-client",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "react-native-audio-api",
        {
          "iosBackgroundMode": false,
          "androidForegroundService": false,
          "iosMicrophonePermission": "Purrrfect Keys uses your microphone to detect the notes you play on piano, keyboard, or by singing."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.619761780367-tqf3t4srqtkklkigep0clojvoailsteu"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5283b6e2-cc46-4067-a759-426d1ea3c388"
      }
    },
    "owner": "ut254",
    "runtimeVersion": "0.1.0",
    "updates": {
      "url": "https://u.expo.dev/5283b6e2-cc46-4067-a759-426d1ea3c388"
    }
  }
}
